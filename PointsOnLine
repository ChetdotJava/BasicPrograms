
public class Coordinates {
    public static void main(String args[]){
        int x1=1, x2=2, x3=3, y1=1, y2=2, y3=3;
        int m=(y2-y1)/(x2-x1);
        int m1= (y3-y2)/(x3-x2);

        if(m==m1){
            System.out.println("points lie on the same line");
        }
        else{
            System.out.println("points are not in the same line");
        }
       
}
}
