import java.util.Scanner;
public class Loop {
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int i=1,factorial=1;
        System.out.println("Enter the no. of which factorial is to be calculated:");
        int fact = sc.nextInt();
        if(fact==0 || fact==1){
            System.out.printf("Factorial is:\t%d",factorial);
        }

        if(fact>1){
            while(i<=fact){
                factorial=factorial*i;
                i++;
            }
            System.out.printf("Factorial is:\t%d",factorial);
        }


    }
}

